#/bin/sh
FailUnexpected=`grep -R "\[X\]" ./**fail** | wc -l`
FailGood=`grep -R "\[O\]" ./**fail** | wc -l`
FailTotal=`grep -R "\[" ./**fail** | wc -l`
echo Fail: $FailUnexpected / $FailGood / $FailTotal

PassUnexpected=`grep -R "\[X\]" ./**pass** | wc -l`
PassGood=`grep -R "\[O\]" ./**pass** | wc -l`
PassTotal=`grep -R "\[" ./**pass** | wc -l`
echo Pass: $PassUnexpected / $PassGood / $PassTotal
